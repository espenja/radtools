{
	"parser": "@typescript-eslint/parser",
	"plugins": ["prettier", "@typescript-eslint"],
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended",
		"plugin:prettier/recommended",
		"plugin:@typescript-eslint/eslint-recommended",
		"prettier/@typescript-eslint"
	],
	"settings": {
		"react": {
			"version": "detect"
		}
	},
	"rules": {
		"no-extra-semi": "error"
	}
}